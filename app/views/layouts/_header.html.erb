<header>
  <div>
    <%= link_to "Member Only app", '#', id: "logo" %>
  </div>
  <nav>
    <ul>
      <li><%= link_to "All Posts", '#' %></li>
      <li><%= link_to "Create Post", '#' %></li>
        <% if logged_in? %>
          <ul>
            <li>
              <%= link_to "Sign out", '#', method: :delete %>
            </li>
          </ul>
      <% else %>
        <li><%= link_to "Sign in", '#' %></li>
      <% end %>
    </ul>
  </nav>
</header>